						"exec": [
							"var jsonData = JSON.parse(responseBody);",
							"tests[\"Store 'test_store' is returned\"] = jsonData.stores[0].name === 'test_store';",
							"tests[\"ID of store 'test_store' is returned\"] = jsonData.stores[0].id === parseInt(environment.store_id);",
